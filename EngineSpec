public class EngineSpec {

    String transmission;

    Boolean isLuxury;



    EngineSpec(){

    }

    EngineSpec(String transmission, Boolean isLuxury){
        this.transmission = transmission;
        this.isLuxury = isLuxury;

    }



    public String getTransmission(){
        return  transmission;
    }
    public void setTransmission(String transmission){
        this.transmission = transmission;
    }



    public Boolean getIsLuxury(){
        return  isLuxury;
    }

    public void setIsVintage(){
        this.isLuxury = isLuxury;
    }


   public void setEngineSpec(String transmission, Boolean isLuxury){
        this.transmission = transmission;
        this.isLuxury = isLuxury;

    }

}
